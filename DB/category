create table category(
	cateid int auto_increment primary key,
    catesort int,
    catename varchar(100),
    cateauth varchar(50),
    categroup varchar(2)
);

select * from category;

insert into category(sortnum, catename, cateauth, categroup) 
values(?, ?, ?, ?);

UPDATE category SET sortnum=?, catename=?, cateauth=?  
WHERE cateid=?;

DELETE FROM category WHERE cateid=?;